{
  "name": "yoanbernabeu/airtable-client-bundle",
  "description": "Simple Client for Airtable API",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Yoan Bernabeu",
      "email": "yoan.bernabeu@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4.3",
    "symfony/http-client": "5.2.*",
    "symfony/config": "^5.2.8",
    "symfony/dependency-injection": "^5.2.8",
    "symfony/http-kernel": "^5.2.8",
    "symfony/serializer": "^5.2",
    "symfony/property-access": "^5.2"
  },
  "autoload": {
    "psr-4": {
      "Yoanbernabeu\\AirtableClientBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Yoanbernabeu\\AirtableClientBundle\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "spatie/phpunit-watcher": "^1.23",
    "pestphp/pest": "^1.3",
    "symfony/framework-bundle": "^5.2",
    "friendsofphp/php-cs-fixer": "^3.0",
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-beberlei-assert": "^0.12",
    "phpstan/phpstan-deprecation-rules": "^0.12",
    "phpstan/phpstan-phpunit": "^0.12",
    "phpstan/phpstan-strict-rules": "^0.12",
    "symfony/phpunit-bridge": "^5.2",
    "symfony/test-pack": "^1.0",
    "symfony/yaml": "^5.2"
  },
  "scripts": {
    "test:all": "./vendor/bin/simple-phpunit",
    "test:typing": "./vendor/bin/phpstan analyse --error-format=checkstyle | cs2pr",
    "test:syntax": "./vendor/bin/php-cs-fixer fix --dry-run --stop-on-violation --using-cache=no"
  }
}
